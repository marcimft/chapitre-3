	keywords = {Cerebrovascular Circulation, Humans, Cerebrospinal Fluid, Adult, Male, Models, Brain, Computer Simulation, Cerebrospinal Fluid Pressure, Biological Clocks, Heart, Reference Values, Biological},
	pages = {483--491}
}

