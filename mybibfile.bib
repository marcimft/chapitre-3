@article{angel_reina_origin_2002,
	title = {The {Origin} of the {Spinal} {Subdural} {Space}: {Ultrastructure} {Findings}},
	volume = {94},
	issn = {0003-2999},
	shorttitle = {The {Origin} of the {Spinal} {Subdural} {Space}},
	url = {https://journals.lww.com/anesthesia-analgesia/fulltext/2002/04000/The_Origin_of_the_Spinal_Subdural_Space_.40.aspx},
	doi = {10.1097/00000539-200204000-00040},
	abstract = {Previous studies of samples from cranial meninges have created doubts about the existence of a virtual subdural space. We examined the ultrastructure of spinal meninges from three human cadavers immediately after death to see whether there is a virtual subdural space at this level. The arachnoid mater had two portions: a compact laminar portion covering the dural sac internal surface and a trabecular portion extending like a spider web around the pia mater. There was a cellular interface between the laminar arachnoid and the internal layer of the dura that we called the dura-arachnoid interface. There was no subdural space in those specimens where the dura mater was macroscopically in continuity with the arachnoid trabecules. In the specimens where the dura mater was separated from the arachnoid, we found fissures in between the neurothelial cells that extended throughout the interface. We hypothesize that the subdural space would have its origin within the dura-arachnoid interface when the neurothelial cells break up, creating in this way a real subdural space.},
	language = {en-US},
	number = {4},
	urldate = {2018-02-19},
	journal = {Anesthesia \& Analgesia},
	author = {Angel Reina, Miguel and De Leon Casasola, Oscar and López, Andrés and Antonio De Andrés, José and Mora, Miguel and Fernández, Agustín},
	month = apr,
	year = {2002},
