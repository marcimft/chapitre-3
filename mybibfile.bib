	pages = {67--75},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\Marc\\Zotero\\storage\\4X595I6V\\Sweetman et al. - 2011 - Three-dimensional computational prediction of cere.pdf:application/pdf;ScienceDirect Full Text PDF:C\:\\Users\\Marc\\Zotero\\storage\\GYAVX9V6\\Sweetman et al. - 2011 - Three-dimensional computational prediction of cere.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\Marc\\Zotero\\storage\\PYI2IF59\\S0010482510001708.html:text/html;ScienceDirect Snapshot:C\:\\Users\\Marc\\Zotero\\storage\\CWN69EGZ\\S0010482510001708.html:text/html}
