	pmcid = {PMC4491523},
	keywords = {Blood Pressure, Humans, Models, Blood Flow Velocity, Computer Simulation, Hemodynamics, 1D modeling, Aorta, aortic pulse wave, Arteries, digital pulse wave, hypertension, Renal Artery, windkessel model, Cardiovascular},
	pages = {H222--234}
}

