@article{rutkowska_patient-specific_2012,
	title = {Patient-{Specific} 3D {Simulation} of {Cyclic} {CSF} {Flow} at the {Craniocervical} {Region}},
	volume = {33},
	copyright = {© 2012 by American Journal of Neuroradiology. Indicates open access to non-subscribers at www.ajnr.org},
