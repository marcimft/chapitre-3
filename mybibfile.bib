	keywords = {Models, Hydrodynamics, Blood Circulation, Elasticity, Fluid friction, Friction, Hemodynamics, Nonlinear Dynamics, One-dimensional modeling, Pressure, Pulse wave propagation, Viscoelasticity, Viscosity, Water, Biological},
	pages = {565--571}
}

