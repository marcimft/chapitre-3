	pmid = {7510047},
	keywords = {Cerebrospinal Fluid, Male, Dura Mater, Subarachnoid Space, Animals, Carbon, Cochlea, Coloring Agents, Lymph Nodes, Lymphatic System, Nasal Cavity, Rats, Staining and Labeling, Wistar},
	pages = {480--488}
}

@incollection{kurtcuoglu_computational_2011,
